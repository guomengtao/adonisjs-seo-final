{
  "name": "temp_adonis-new",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build --ignore-ts-errors && npm run build:css",
    "build:css": "mkdir -p dist && touch dist/localtailwind.css",
    "dev": "npm run build:css && node ace serve --hmr",
    "dev:css": "npx tailwindcss -i ./resources/css/app.css -o ./dist/localtailwind.css --watch",
    "serve": "npm run build:css && npx http-server -p 3000 -o",
    "serve:prod": "npm run build && npx http-server build -p 3000 -o",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#templates/*": "./app/templates/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.4",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/api-client": "^3.1.0",
    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@neondatabase/serverless": "^1.0.2",
    "@swc/core": "1.11.24",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.15.18",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.26.0",
    "hot-hook": "^0.4.0",
    "http-server": "^14.1.1",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.18",
    "ts-node-maintained": "^10.9.5",
    "typescript": "~5.8",
    "wrangler": "^4.59.3"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^21.6.1",
    "@adonisjs/static": "^1.1.1",
    "@aws-sdk/client-s3": "^3.968.0",
    "@google/generative-ai": "^0.24.1",
    "@huggingface/hub": "^2.7.2",
    "@libsql/client": "^0.17.0",
    "@libsql/sqlite3": "^0.3.1",
    "@rlanz/bull-queue": "^3.1.0",
    "@supabase/supabase-js": "^2.91.1",
    "@vinejs/vine": "^3.0.1",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.6.0",
    "cheerio": "^1.2.0",
    "drizzle-orm": "^0.45.1",
    "edge.js": "^6.4.0",
    "jsdom": "^27.4.0",
    "luxon": "^3.7.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.17.2",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.5",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config",
  "main": "ace.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guomengtao/adonisjs-seo-final.git"
  },
  "keywords": [],
